{
    "manifest_version": 3,
    "name": "Jack's Handy Pageblocker",
    "description": "Don't Use :", 
    "version": "42.0.0",
    "author": "you ;",
    "permissions": [
        "tabs",
        "webNavigation",
        "scripting",
        "storage"
    ],
    "host_permissions": ["*://*/*"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }

}